<Page x:Class="Sunrise.MultipleChoice.Register"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sunrise.MultipleChoice"
      mc:Ignorable="d"
      Title="Register" Height="394" Width="432">

    <Grid Margin="0,0,-190,-153">
        <Canvas Background="LightSkyBlue">
            <Image x:Name="Login_image" Height="176" Canvas.Left="18" Canvas.Top="11" Width="254" Source="images/Login_Images/Login_questionnaire.jpg"/>
            <Button x:Name="Login_exit_button" Content="" Canvas.Left="580" Canvas.Top="10" Width="27" Height="26" BorderBrush="White" Click="Login_exit_button_Click" IsCancel="True">
                <Button.Background>
                    <ImageBrush ImageSource="images/Login_Images/Login_exit.png" TileMode="Tile"/>
                </Button.Background>
            </Button>
        </Canvas>

    </Grid>
</Page>
